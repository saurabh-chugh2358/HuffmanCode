package sorting.LinearSort;

public class LinearSortTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		LinearSort ls = new LinearSort();
		for (int i = 0; i < ls.linearArray.length; i++) {
			System.out.println(ls.linearArray[i]);
		}
		
		ls.insertionSort(ls.linearArray, ls.linearArray.length);
		
		for (int i = 0; i < ls.linearArray.length; i++) {
			System.out.println(ls.linearArray[i]);
		}
		
		ls.setMaxSize(50);
		ls.shellSort();
		
	}
}
